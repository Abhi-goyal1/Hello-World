}
#timeline .demo-card--step4 {
  background-color: #000;
}
#timeline .demo-card--step4 .head::after {
  border-color: #000;
}
#timeline .demo-card--step5 {
  background-color:  #2baffc;
}
#timeline .demo-card--step5 .head::after {
  border-color:  #2baffc;
}



#more-tutorials i {
